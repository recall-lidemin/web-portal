{
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev umi dev --port 9001",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev --port 9000",
    "build:stage": "cross-env REACT_APP_ENV=staging umi build",
    "build:test": "cross-env REACT_APP_ENV=testing umi build",
    "build:prod": "cross-env REACT_APP_ENV=production umi build",
    "test": "umi test",
    "lint": "eslint {src,mock,tests}/**/*.{ts,tsx} --fix",
    "precommit": "lint-staged"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@ant-design/pro-table": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.16",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "koa": "^2.13.0",
    "koa-compress": "^5.0.1",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.0.3",
    "umi": "^3.2.16",
    "umi-server": "^1.2.3",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "babel-eslint": "^9.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^10.0.7"
  }
}
